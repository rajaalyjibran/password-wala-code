	char name[12];

cout<<"Enter your password ";
cin>>password;	

	if (password==1234)
	
	{
		cout<<"password is correct\n";
	}

   else 
   {
   	cout<<"incorrect password"<<endl;
   }
   
   cout<<"Enter your name please \n ";
